{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "asantibanez/livewire-charts": "^2.4.1",
        "astrotomic/laravel-translatable": "^11.11",
        "barryvdh/laravel-dompdf": "^2.0",
        "bepsvpt/secure-headers": "^7.3",
        "binarytorch/larecipe": "^2.5",
        "davmixcool/laravel-meta-manager": "^1.2",
        "dietercoopman/smart": "^1.6.5",
        "diglactic/laravel-breadcrumbs": "^8.0.1",
        "guzzlehttp/guzzle": "^7.5",
        "jorenvanhocht/laravel-share": "^4.2",
        "kirschbaum-development/eloquent-power-joins": "^3.0",
        "lab404/laravel-impersonate": "^1.7.3",
        "laravel/framework": "^9.46",
        "laravel/sanctum": "^3.1",
        "laravel/tinker": "^2.7.3",
        "livewire/livewire": "^2.10.8",
        "mediconesystems/livewire-datatables": "^0.10.0",
        "opcodesio/log-viewer": "^2.4",
        "outhebox/laravel-translations": "^0.0.6",
        "owen-it/laravel-auditing": "^13.0.5",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/laravel-cookie-consent": "^3.2.2",
        "spatie/laravel-medialibrary": "^10.7.8",
        "tanmuhittin/laravel-google-translate": "^2.1",
        "wire-elements/modal": "^1.0.7",
        "wireui/wireui": "^1.17.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "beyondcode/laravel-query-detector": "^1.6",
        "fakerphp/faker": "^1.21",
        "glhd/laravel-dumper": "^1.0.1",
        "imanghafoori/laravel-microscope": "^1.0.278",
        "laravel/breeze": "^1.17",
        "laravel/pint": "^1.3",
        "laravel/sail": "^1.17",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4",
        "phpunit/phpunit": "^9.5.27",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
